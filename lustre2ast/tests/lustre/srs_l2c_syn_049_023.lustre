function foldi_test1(
   Input1 : int; 
   Input2 : int ^5)
returns (
   Output1 : int)
var
   _L1 : int;
   _L2 : int;
   _L5 : int ^5;
let
   _L1 = ( foldi sum_three_one<<5>>)(_L2, _L5);
   _L2 = Input1;
   Output1 = _L1;
   _L5 = Input2;
tel

function sum_three_one(
   Input1 : int; 
   Input2 : int; 
   Input3 : int)
returns (
   Output1 : int)
var
   _L1 : int;
   _L2 : int;
   _L3 : int;
   _L4 : int;
let
   _L1 = _L2 + _L3 + _L4;
   _L2 = Input1;
   _L3 = Input2;
   _L4 = Input3;
   Output1 = _L1;
tel
