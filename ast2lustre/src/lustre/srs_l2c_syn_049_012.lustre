   t : int ^10)
var
   aux : int;
   _L1 : int;
   _L2 : int;
   _L3 : int ^10;
   _L4 : int ^10;
let
   _L1, _L4 = ( mapfold sum_dup<<10>>)(_L2, _L3);
   _L2 = 0;
   _L3 = 2 ^ 10;
   aux = _L1;
